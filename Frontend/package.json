{
  "name": "trident-forms",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "tsc",
    "test": "ng test",
    "lint": "eslint --fix './src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "prebuild": "npm run lint && npm run format",
    "e2e": "ng e2e",
    "clean": "rm -rf dist target",
    "build:local": "ng build --output-path=dist/local",
    "start:local": "ng serve",
    "start:dev": "ng serve -c=dev",
    "start:qa": "ng serve -c=qa",
    "start:uat": "ng serve -c=uat",
    "start:test": "ng serve -c=test",
    "start:prod": "ng serve -c=production",
    "build:dev": "ng build -c=dev --output-path=dist/dev",
    "build:qa": "ng build -c=qa --output-path=dist/qa",
    "build:uat": "ng build -c=uat --output-path=dist/uat",
    "build:test": "ng build -c=test --output-path=dist/test",
    "build:prod": "ng build -c=production --output-path=dist/prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.0",
    "@angular/cdk": "^11.2.12",
    "@angular/common": "~12.0.0",
    "@angular/compiler": "~12.0.0",
    "@angular/core": "~12.0.0",
    "@angular/forms": "~12.0.0",
    "@angular/material": "^11.2.12",
    "@angular/material-moment-adapter": "^11.2.12",
    "@angular/platform-browser": "~12.0.0",
    "@angular/platform-browser-dynamic": "~12.0.0",
    "@angular/router": "~12.0.0",
    "moment": "^2.29.1",
    "ngx-take-until-destroy": "^5.4.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.0",
    "@angular-eslint/builder": "0.8.0-beta.3",
    "@angular-eslint/eslint-plugin": "0.8.0-beta.3",
    "@angular-eslint/eslint-plugin-template": "0.8.0-beta.3",
    "@angular-eslint/schematics": "^12.0.0",
    "@angular-eslint/template-parser": "0.8.0-beta.3",
    "@angular/cli": "^12.0.0",
    "@angular/compiler-cli": "~12.0.0",
    "@types/jasmine": "^3.6.11",
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^12.20.13",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.23.0",
    "@typescript-eslint/parser": "4.3.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-prettier": "^3.4.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "prettier": "^2.3.0",
    "prettier-eslint": "^12.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4"
  }
}
